#include <bits/stdc++.h>

using namespace std;

int N, H, L;
vector<int> horror;
vector<vector<int>> alist;
vector<int> HI;
int INF = 1e9;

int main(){

	cin >> N >> H >> L;
	
	horror = vector<int>(H);
	for (int& x: horror) cin >> x;

	alist = vector<vector<int>>(N);
	while (L--) {
		int a, b;
		cin >> a >> b;

		alist[a].push_back(b);
		alist[b].push_back(a);
	}

	queue<int> q;
	HI = vector<int>(N, INF);
	for (int x: horror) {
		q.push(x);
		HI[x] = 0;
	}

	while (!q.empty()) {
		int u = q.front(); q.pop();

		for (int v: alist[u]) {

			if (HI[v] < HI[u]+1) continue;

			HI[v] = HI[u]+1;
			q.push(v);
		}
	}


	int maxhi = 0;
	int ans ;
	for (int i=0; i<N; i++) {
		if (maxhi >= HI[i]) continue;

		maxhi = HI[i];
		ans = i;
	}

	cout << ans << endl;

	return 0;
}
